        <div> GroupName2 Branch no. {{$i}}</div>
        {{range .MealPeriod}}
            <div> GroupName3 MealPeriod {{.Name}} </div>
            {{range .Item}}
                <div> {{.Name}}, stocks {{.Quantity}} </div>
            {{end}}
        {{end}}
        {{$i = add $i 1}}
    {{end}}
</body>
</html>